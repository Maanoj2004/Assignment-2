#include <stdio.h>

int main() {
    int intArr[5] = {12, 56, 34, 78, 100};
    int largest = intArr[0]; // assume the first element is the largest
    
    for (int i = 1; i < 5; i++) {
        if (intArr[i] > largest) {
            largest = intArr[i];
        }
    }
    
    printf("The largest element is %d\n", largest);
    
    return 0;
}
